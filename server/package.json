	"license": "ISC",
	"dependencies": {
		"aws-lambda-fastify": "^1.4.4",
		"fastify": "^3.14.0",
		"fastify-cors": "^5.2.0",
		"fastify-jwt": "^2.3.0",
		"fastify-plugin": "^3.0.0",
		"fastify-sensible": "^3.1.0",
		"got": "^11.8.2",
		"graphql": "^15.5.0",
		"luxon": "^1.26.0",
		"mercurius": "^7.2.0",
		"nexus": "^1.0.0",
		"plaid": "^8.2.0"
	},
	"devDependencies": {
		"dotenv": "^8.2.0",
		"nodemon": "^2.0.7",
		"npm-run-all": "^4.1.5"
	}
